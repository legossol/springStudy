package com.example.demo.bbs.domain;

import javax.persistence.*;

@Entity
@Table(name="boards")
public class Board {
	@Column(name = "board_no")
	private long boardNo;
	@Column(name = "title")
	private String title;
	@Column(name = "index")
	private String index;
	@Column(name = "writer")
	private String writer;
	@Column(name = "date")
	private String date;
}
